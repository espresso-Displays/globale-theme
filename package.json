{
  "name": "globale-theme",
  "version": "1.0.0",
  "description": "[![Build status](https://github.com/shopify/dawn/actions/workflows/ci.yml/badge.svg?branch=main)](https://github.com/Shopify/dawn/actions/workflows/ci.yml?query=branch%3Amain) [![PRs Welcome](https://img.shields.io/badge/PRs-welcome-brightgreen.svg?color=informational)](/.github/CONTRIBUTING.md)",
  "main": "tailwind.config.js",
  "scripts": {
    "build:css": "tailwindcss -c tailwind.config.js -i ./assets/tailwind.css -o ./assets/style.css --minify",
    "watch:css": "tailwindcss -c tailwind.config.js -i ./assets/tailwind.css -o ./assets/style.css --watch",
    "dev": "concurrently \"npm run watch:css\" \"shopify theme dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1"
  },
  "devDependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17"
  }
}

          class="header__icon header__icon--cart link focus-inset w-6 h-6 flex items-center justify-center"
          id="cart-icon-bubble"
        >
          {%- liquid
            render 'icon-cart'
          -%}

          {%- if cart != empty -%}
            <div class="cart-count-bubble">
              {%- if cart.item_count < 100 -%}
                <div class=" relative w-0 h-0 pointer-events-none">
                  <div class="badge bg-none absolute top-[0px] right-[1px] border-none w-[12px]  z-10 ">
                    <div class="text-lm-on-background-1 text-center w-full text-[8px] font-[600]">
                      {{ cart.item_count }}
                    </div>
                    <svg
                      width="12"
                      height="12"
                      viewBox="0 0 11 11"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                      class="relative bottom-[17px] lg:bottom-[19px] z-[-1] pointer-events-none"
                    >
                      <rect width="11" height="11" fill="none"></rect><circle id="Ellipse 48" cx="5.5" cy="5.5" r="5.5" fill="white"></circle>
                    </svg>
                  </div>
                </div>
                {% comment %} <span aria-hidden="true">{{ cart.item_count }}</span> {% endcomment %}
              {%- endif -%}
              <span class="visually-hidden hidden">
                {{- 'sections.header.cart_count' | t: count: cart.item_count -}}
              </span>
            </div>
          {%- endif -%}
        </button>
      </div>
    </div>
